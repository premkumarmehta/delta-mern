<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>users</title>
</head>
<style>
    table, tr, td, th{
        border: 1px solid black;
    }
</style>
<body>
    <h2>All user Details : </h2>

    <table>
        <tr>
            <th>S.no</th>
            <th>Id</th>
            <th>username</th>
            <th>Email</th>
        </tr>

        <% let count = 1; %>
        <% for(user of users) { %>
        
        <tr>
            <td><%= count++ %></td>
            <td><%= user.id %></td>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td>
                <form method="GET" action="/user/<%= user.id %>/edit">
                    <button>Edit username</button>
                </form>
            </td>
           
        </tr>

        <% } %>
   
    </table>

    
    
</body>
</html>